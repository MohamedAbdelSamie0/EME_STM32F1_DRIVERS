  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif

#include "EXTI_interface.h"
#include "GPIO_interface.h"
#include "RCC_interface.h"


int main(void)
{
	RCC_voidInitSysClock();
	RCC_voidEnableClock(RCC_APB2, APB2_GPIOA_EN);
	GPIO_voidSetPinPull(GPIO_A, PIN_4, PULL_UP);
	GPIO_voidSetPinDirection(GPIO_A, PIN_4, INPUT_PULL_UP);
	GPIO_voidSetPinDirection(GPIO_A, PIN_2, OUTPUT_SPEED_2MHZ_AF_PUSH_PULL);
	/* Loop forever */
	for(;;);
}
